@model PlanillaCabecera
@{
    Layout = null;
    //Layout = "_LayoutForUsers";
}

<div class="row">
     <div class="col-md-12">
         <form asp-action="EditItem" onsubmit="return jQueryAjaxPost(this);">               
             <div class="modal-body form-horizontal">  

                 <div class="form-group row">  

                     @for(int i=0; i<Model.Registros_Json?.Count; i++)
                     {
                         <input class="form-control" value="@Model.Registros_Json[i]"/>
                     }

                 </div> 
                 
             </div>
         </form>
    </div>
</div>


